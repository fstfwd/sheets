{
   "name": "@sheetbase/sheets-server",
   "version": "0.0.6",
   "description": "Using Google Sheets as a database.",
   "author": "Sheetbase",
   "homepage": "https://sheetbase.net",
   "license": "MIT",
   "main": "dist/bundles/sheetbase-sheets-server.umd.js",
   "module": "dist/fesm3/sheetbase-sheets-server.js",
   "typings": "dist/sheetbase-sheets-server.d.ts",
   "files": [
      "dist"
   ],
   "scripts": {
      "module": "sheetbase-app-scripts build --module",
      "readme": "sheetbase-app-scripts readme",
      "docs": "sheetbase-app-scripts docs",
      "build": "npm run module && npm run readme && npm run docs",
      "push": "cd deploy && clasp push",
      "lint": "tslint --project tslint.json && echo 'All files pass linting.'",
      "test": "tsc --project tests/tsconfig.test.json && nyc --cache false mocha --timeout 100000 -- build/tests/*.js",
      "coverage": "nyc --cache false report --reporter=text-lcov | coveralls"
   },
   "nyc": {
      "exclude": [
         "**/tests",
         "**/src/lodash",
         "**/src/lunr"
      ]
   },
   "keywords": [
      "google-apps-script",
      "sheets",
      "sheetbase",
      "database",
      "nosql"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/sheetbase/sheets-server.git"
   },
   "bugs": {
      "url": "https://github.com/sheetbase/sheets-server/issues"
   },
   "dependencies": {
      "@sheetbase/tamotsux-server": "0.0.4"
   },
   "peerDependencies": {
      "@sheetbase/core-server": "0.0.19"
   },
   "devDependencies": {
      "@sheetbase/app-scripts": "^0.0.30",
      "@types/chai": "^4.1.7",
      "@types/google-apps-script": "^0.0.32",
      "@types/mocha": "^5.2.5",
      "@types/node": "^10.12.8",
      "@types/sinon": "^5.0.7",
      "chai": "^4.2.0",
      "coveralls": "^3.0.2",
      "mocha": "^5.2.0",
      "nyc": "^13.1.0",
      "rollup-plugin-node-resolve": "^3.4.0",
      "sinon": "^7.1.1",
      "tslint": "^5.11.0",
      "typescript": "^3.1.6"
   }
}
